{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"sourceMap": true,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"removeComments": false,
		"noImplicitAny": false
	},
	"compileOnSave": true,
	"exclude": [
		"node_modules",
		"platforms"
	]
}